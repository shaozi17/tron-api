{
  "name": "shaozi17/tron-api",
  "description": "A PHP API for interacting with Tron (Trx),support Stake2.0",
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/shaozi17/tron-api",
  "authors": [
    {
      "name": "Shamsudin Serderov",
      "email": "steein.shamsudin@gmail.com"
    },
    {
      "name": "ufado",
      "email": "ufiles@proton.me"
    }
  ],
  "keywords": [
    "ufado",
    "iexbase",
    "tron-lib",
    "tron-php",
    "tron-api",
    "tron-rest-api"
  ],
  "require": {
    "php": "^7.2|^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "web3p/web3.php": "^0.3.2",
    "kornrunner/secp256k1": "^0.1.2",
    "simplito/elliptic-php": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0.0"
  },
  "autoload": {
    "psr-4": {
      "IEXBase\\TronAPI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IEXBase\\TronAPI\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
